{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    describe,
    it,
    afterEach,
    beforeEach
  },
  "ecmaFeatures": {
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "destructuring": true,
    "arrowFunctions": true
  },
  "rules": {
    "quotes": 0,
    "camelcase": 0,
    "consistent-return": 0,
    "no-multi-str": 0,
    "no-path-concat": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "no-unused-vars": 1,
    "no-unused-expressions": 0,
    "no-mixed-requires": 0,
    "curly": 0,
    "eqeqeq": 0
  }
}
